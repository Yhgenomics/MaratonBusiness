@model MaratonBusiness.Models.CFService
@{
    ViewBag.Title = "index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<ol class="breadcrumb">
    <li><a href="/">Home</a></li>
    <li><a href="@Url.Action("index","service")">Service</a></li>
</ol>  

<div class="row">
    <div class="col-md-12">
        <form class="form-horizontal" method="post">
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">Maraton Address</label>
                <div class="col-sm-10">

                    @Html.TextBoxFor(m => Model.MaratonAddress , new { @class = "form-control" })
                    @*<input type="text" value="@Model.MaratonAddress" name class="form-control" id="inputEmail3" placeholder="Maraton Address">*@
                </div>
            </div>

            <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">Maraton Port</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(m => Model.MaratonPort, new { @class = "form-control" })
                </div>
            </div>
            <hr />

            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">Meloton Address</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(m => Model.MelotonAddress, new { @class = "form-control" })
                </div>
            </div>

            <div class="form-group">
                <label for="inputPassword3" class="col-sm-2 control-label">Meloton Port</label>
                <div class="col-sm-10">
                    @Html.TextBoxFor(m => Model.MelotonPort, new { @class = "form-control" })
                </div>
            </div>
            <hr />
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-info">Save</button>
                </div>
            </div>
        </form>
    </div>
</div>
