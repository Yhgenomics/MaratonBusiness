@model IEnumerable<MRTBusiness.Models.DbPipe>

@{
    Layout = null;
}
<div class="row">
    <div class="col-lg-12">
        <table class="table table-hover">
            <thead>
                <tr>
                    <td>
                        Name
                    </td>
                    <td>
                        Executor
                    </td> 
                    <td>
                    </td>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @item.Name
                            </td>
                            <td>
                                @item.Executor
                            </td>
                             
                            <td>
                                <a href="javascript:addPipe({ id : '@item.Id', name : '@item.Name'})" class="btn btn-info">Select</a>
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>
